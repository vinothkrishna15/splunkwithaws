	<mapping map-id="customer-mapping" wildcard="false">
		<class-a>com.tcs.destination.bean.CustomerMasterT</class-a>
  		<class-b>com.tcs.destination.bean.dto.CustomerMasterDTO</class-b>
		<field>
			<a>customerId</a>
			<b>customerId</b>
		</field>
		<field>
			<a>customerName</a>
			<b>customerName</b>
		</field>

		<field>
			<a>geographyMappingT.geography</a>
			<b>geographyMappingT.geography</b>
		</field>
		<field>
			<a>geographyMappingT.displayGeography</a>
			<b>geographyMappingT.displayGeography</b>
		</field>
		
	</mapping>
	
