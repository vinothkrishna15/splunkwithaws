	</mapping>
	
	<mapping map-id="competitor-opportunity-link" wildcard="false">
		<class-a>com.tcs.destination.bean.OpportunityCompetitorLinkT</class-a>
  		<class-b>com.tcs.destination.bean.dto.OpportunityCompetitorLinkDTO</class-b>
  			
