<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
          
	<configuration >
	  <date-format>MM/dd/yyyy HH:mm</date-format>
	  <wildcard>true</wildcard>
	  <trim-strings>false</trim-strings>
	</configuration>
	
	
	<mapping map-id="opportunity-winloss" wildcard="false">
		<class-a>com.tcs.destination.bean.OpportunityT</class-a>
  		<class-b>com.tcs.destination.bean.dto.OpportunityDTO</class-b>
  		
		<field><a>opportunityId</a><b>opportunityId</b></field>
		<field><a>opportunityName</a><b>opportunityName</b></field>
		<field><a>dealClosureDate</a><b>dealClosureDate</b></field>
	  	<field><a>digitalDealValue</a><b>digitalDealValue</b></field>
		<field><a>salesStageCode</a><b>salesStageCode</b></field>
		
		<field><a>customerMasterT.customerId</a><b>customerMasterT.customerId</b></field>
		<field><a>customerMasterT.customerName</a><b>customerMasterT.customerName</b></field>
		<field><a>customerMasterT.groupCustomerName</a><b>customerMasterT.groupCustomerName</b></field>
		<field><a>customerMasterT.geographyMappingT.geography</a><b>customerMasterT.geographyMappingT.geography</b></field>
		<field><a>customerMasterT.geographyMappingT.displayGeography</a><b>customerMasterT.geographyMappingT.displayGeography</b></field>
		<field><a>customerMasterT.iouCustomerMappingT.iou</a><b>customerMasterT.iouCustomerMappingT.iou</b></field>
		<field><a>customerMasterT.iouCustomerMappingT.displayIou</a><b>customerMasterT.iouCustomerMappingT.displayIou</b></field>
	</mapping>
	
